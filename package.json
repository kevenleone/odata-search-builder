{
    "author": {
        "name": "Keven Leone",
        "email": "keven.santos.sz@gmail.com"
    },
    "description": "A lightweight and flexible TypeScript library for building OData filter queries.",
    "devDependencies": {
        "@types/bun": "latest"
    },
    "exports": {
        ".": "./dist/index.js"
    },
    "files": [
        "dist",
        "LICENSE.md",
        "README.md"
    ],
    "keywords": [
        "odata",
        "odata-filter",
        "odata-query-builder"
    ],
    "module": "src/index.ts",
    "name": "odata-search-builder",
    "peerDependencies": {
        "typescript": "^5"
    },
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kevenleone/odata-search-builder.git"
    },
    "type": "module",
    "scripts": {
        "build": "bun build ./src/*.ts --outdir ./dist --target node --root ./src && tsc --emitDeclarationOnly --declaration --outDir ./dist ./src/index.ts"
    },
    "version": "1.1.2"
}
